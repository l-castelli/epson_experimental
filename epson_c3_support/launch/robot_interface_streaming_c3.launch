<!--
  Manipulator specific version of 'robot_interface_streaming.launch'.

  Defaults provided for C3:
   - 6 joints

  Usage:
    robot_interface_streaming_c3.launch robot_ip:=<value>
-->
<launch>
  <arg name="robot_ip" />

  <rosparam command="load" file="$(find epson_c3_support)/config/joint_names_c3.yaml" />

  <include file="$(find epson_spelp_driver)/launch/robot_interface_streaming.launch">
    <arg name="robot_ip" value="$(arg robot_ip)" />
  </include>
</launch>
